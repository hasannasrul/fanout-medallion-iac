# Example terraform.tfvars for prod environment
# Copy this to terraform.tfvars and update with your values

environment           = "prod"
aws_region            = "us-east-1"
project_name          = "fanout-medallion"

# RDS Configuration (Production - not free tier)
rds_engine            = "postgres"
rds_instance_class    = "db.t3.small"
rds_database_name     = "analytics"
rds_username          = "admin"
rds_password          = "YourSecureProductionPassword123!"  # CHANGE THIS!
rds_allocated_storage = 100

# S3 & SQS Configuration
s3_versioning_enabled = true
enable_logging        = true
lambda_timeout        = 120
lambda_memory         = 512
sqs_message_retention_seconds = 1209600  # 14 days
sqs_visibility_timeout_seconds = 300     # 5 minutes

